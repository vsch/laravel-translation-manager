{
    "name": "vsch/laravel-translation-manager",
    "description": "Enhanced Laravel Translation Manager",
    "keywords": [
        "laravel",
        "translations",
        "translator"
    ],
    "homepage": "https://github.com/vsch/laravel-translation-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Schneider",
            "email": "vladimir.schneider@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "illuminate/support": "^7.0",
        "laravelcollective/html": "^6.1",
        "illuminate/translation": "^7.2",
        "doctrine/dbal": "^2.10",
        "symfony/finder": "^5.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~6.1",
        "fzaninotto/faker": "~1.9",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "src/Classes"
        ],
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Vsch\\TranslationManager\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
        }
    },
    "scripts": {
        "post-update-cmd": [
        ]
    },
    "minimum-stability": "stable"
}
